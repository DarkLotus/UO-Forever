<Gump closable="true" disposable="true" dragable="false" resizable="true">
	<VBox padding="16" backgroundID="3600" FitToContents="true">
		<Label hue="1153">"You are currently on "+objs.group.captain.RawName+"'s team."</Label>
		<HBox>
			<Button normalID="5540" pressedID="5542" name="helpGump"/>
			<Label hue ="1553">"See the instructions (YOU ARE RESPONSIBLE FOR THESE!)"</Label>
		</HBox>
		<HBox>
			<Button normalID="5540" pressedID="5542" name="templateShowScreen"/>
			<Label hue ="1553">"Show me the available templates."</Label>
		</HBox>
		<HBox>
			<Button normalID="5540" pressedID="5542" name="scoreboard"/>
			<Label hue ="1553">"See the scoreboard."</Label>
		</HBox>
		<Spacer height="10"/>
		<Label hue ="1553">"Your Team:"</Label>
		<List list="objs.group.members" objs="tempTeamMob">
			<HBox>
				<Image x="5" gumpID="5032"/>
				<Label hue="1153">objs.tempTeamMob.rawname</Label>
			</HBox>
		</List>
		<Spacer height="20"/>
		<HBox>
			<Button normalID="5540" pressedID="5542" name="quitTeam"/>
			<Label hue ="1553">"Quit Team"</Label>
		</HBox>
		
	</VBox>
</Gump>